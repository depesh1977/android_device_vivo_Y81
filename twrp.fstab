# Android fstab file.
# The filesystem that contains the filesystem checker binary (typically /system) cannot
# specify MF_CHECK, and must come before any filesystems that do specify MF_CHECK

# Currently we dont have e2fsck compiled. So fs check would failed.

# <mount_point>  <fstype>  <device>        <device2>                  <flags>
/system               ext4     /dev/block/platform/bootdevice/by-name/system        flags=display="System";backup=1;wipeingui
/system_image 	      emmc	   /dev/block/platform/bootdevice/by-name/system        flags=display=System Image;backup=1;flashimg=1
/oem                 ext4     /dev/block/platform/bootdevice/by-name/cust          flags=display=Cust;backup=1;flashimg=1
/oem_image           emmc     /dev/block/platform/bootdevice/by-name/cust           flags=display="Cust Image";flashimg=1;backup=1
/vendor               ext4     /dev/block/platform/bootdevice/by-name/vendor        flags=display="Vendor";backup=1;wipeingui
/vendor_image         emmc     /dev/block/platform/bootdevice/by-name/vendor        flags=display=Vendor Image;backup=1;flashimg=1
/data		              ext4	   /dev/block/platform/bootdevice/by-name/userdata      flags=display=Data;encryptable=/dev/block/platform/bootdevice/by-name/metadata
/cache                ext4     /dev/block/platform/bootdevice/by-name/cache         flags=backup=1;wipeingui;wipeduringfactoryreset

/metadata             emmc     /dev/block/platform/bootdevice/by-name/metadata
/protect_f            ext4     /dev/block/platform/bootdevice/by-name/protect1      flags=backup=1
/protect_s            ext4     /dev/block/platform/bootdevice/by-name/protect2      flags=backup=1
/nvdata               ext4     /dev/block/platform/bootdevice/by-name/nvdata        flags=display=NVdata;backup=1;flashimg=1
/nvcfg                ext4     /dev/block/platform/bootdevice/by-name/nvcfg         flags=display=NVcfg;backup=1;flashimg=1
/persist_image        emmc     /dev/block/platform/bootdevice/by-name/persist       flags=display="Persist Image";backup=1
/persist              ext4     /dev/block/platform/bootdevice/by-name/persist       flags=display=Persist;backup=1;flashimg=1
/persistent           emmc     /dev/block/platform/bootdevice/by-name/frp           
/nvram                emmc     /dev/block/platform/bootdevice/by-name/nvram         flags=display=NVram;backup=1;flashimg=1
/proinfo              emmc     /dev/block/platform/bootdevice/by-name/proinfo       flags=display=Proinfo;backup=1;flashimg=1
/bootloader           emmc     /dev/block/platform/bootdevice/by-name/lk            flags=display=Bootloader(lk);backup=1;flashimg=1
/bootloader2          emmc     /dev/block/platform/bootdevice/by-name/lk2           flags=display=Bootloader2(lk2);backup=1;flashimg=1
/para                 emmc     /dev/block/platform/bootdevice/by-name/para          
/boot                 emmc     /dev/block/platform/bootdevice/by-name/boot          flags=display=Boot;backup=1;flashimg=1
/recovery             emmc     /dev/block/platform/bootdevice/by-name/recovery      flags=display=Recovery;backup=1;flashimg=1
/logo                 emmc     /dev/block/platform/bootdevice/by-name/logo          flags=display=Logo;backup=1;flashimg=1
/seccfg               emmc     /dev/block/platform/bootdevice/by-name/seccfg        flags=display=Seccfg;backup=1;flashimg=1
/tee1                 emmc     /dev/block/platform/bootdevice/by-name/tee1          flags=display="Trustzone1";backup=1
/tee2                 emmc     /dev/block/platform/bootdevice/by-name/tee2          flags=display="Trustzone2";backup=1
/efuse                emmc     /dev/block/platform/bootdevice/by-name/efuse         flags=display=Efuse;backup=1;flashimg=1
/boot_para            emmc     /dev/block/platform/bootdevice/by-name/boot_para     flags=display=Boot_para;backup=1;flashimg=1
/odmdtbo              emmc     /dev/block/platform/bootdevice/by-name/odmdtbo       flags=display=Odmdtbo;backup=1;flashimg=1

/external_sd      vfat     /dev/block/mmcblk0p1  /dev/block/mmcblk0            flags=display="external_sd";storage;wipeingui;removable;fsflags="utf8"
/usb-otg          vfat     /dev/block/sdd1  /dev/block/sdd                     flags=display="usb-otg";storage;wipeingui;removable;fsflags="utf8"
