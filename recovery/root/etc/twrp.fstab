# mount point    fstype    device                       device2               flags
/system               ext4     /dev/block/platform/bootdevice/by-name/system        flags=backup=1
/system_image 	      emmc	   /dev/block/platform/bootdevice/by-name/system        flags=display=system_image;backup=1;flashimg=1
/vendor               ext4     /dev/block/platform/bootdevice/by-name/vendor        flags=backup=1
/vendor_image         emmc     /dev/block/platform/bootdevice/by-name/vendor        flags=display=vendor_image;backup=1;flashimg=1
/data                 ext4     /dev/block/platform/bootdevice/by-name/userdata      flags=forceencrypt=/dev/block/platform/bootdevice/by-name/metadata
/boot                 emmc     /dev/block/platform/bootdevice/by-name/boot          flags=display=boot;backup=1;flashimg=1
/recovery             emmc     /dev/block/platform/bootdevice/by-name/recovery      flags=display=recovery;backup=1;flashimg=1
/seccfg               emmc     /dev/block/platform/bootdevice/by-name/seccfg        flags=backup=1;flashimg=1

# Removable storage
/external_sd     auto      /dev/block/mmcblk1p1         /dev/block/mmcblk1    flags=display="MicroSD Card"
/usb-otg         auto      /dev/block/sda1              /dev/block/sda        flags=display="USB OTG";storage;wipeingui;removable
